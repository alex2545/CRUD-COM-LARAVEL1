00 - BD
CREATE DATABASE BLOG;
CREATE TABLE USUARIO(
id int AUTO_INCREMENT,
    nome varchar(60)not null,
    email varchar(60)not null,
    cpf varchar(14)not null,
    descricao varchar(255)not null,
    preco float(8,2)not null,
    primary key(id)

);